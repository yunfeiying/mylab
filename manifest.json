{
  "manifest_version": 3,
  "key": "",
  "default_locale": "en",
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "3.2.0",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "scripting",
    "identity"
  ],
  "host_permissions": [
    "https://dav.jianguoyun.com/*",
    "https://*.googleapis.com/*",
    "<all_urls>",
    "https://api.lemonsqueezy.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "tesseract.min.js",
        "lib/Readability.js",
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "match_origin_as_fallback": true
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "oauth2": {
    "client_id": "377476964353-tomif4h5t501p0pc3m9aukfo9ccca0pb.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "pdf_viewer.html",
        "tesseract.min.js",
        "worker.min.js",
        "tesseract-core.wasm.js",
        "tesseract-core.wasm",
        "ocr.html",
        "*.gz"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "sandbox": {
    "pages": [
      "ocr.html"
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; worker-src 'self';",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval' blob:; child-src 'self' blob:; worker-src 'self' blob:;"
  }
}